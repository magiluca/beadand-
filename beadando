    for str in a:
        if str=='2' or str=='4' or str== '6' or str == '8' or str == '1' or str == '3' or str == '5' or str == '7' or str == '9':
            str=int(str)
            if str%2==0:
                str=str.str()
                return True
            else:
                return False


def paratlanszamok(a):
    for str in a:
        if str == '1' or str == '3' or str == '5' or str == '7' or str == '9' or str=='2' or str=='4' or str== '6' or str == '8':
            str = int(str)
            if str % 2 != 0:
                str = str.str()
                return True
            else:
                return False

