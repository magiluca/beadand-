    for str in a:
        if '0' == str :
            return True
        else:
            return False

def ujlista(a):
    ls=[]
    for i in a:
        if kisbetu(i) == True:
            ls.append(i)
    for i in a:
        if nagybetu(i)==True:
            ls.append(i)
    for i in a:
        if parosszamok(i) == True:
            ls.append(i)
    for i in a:
        if paratlanszamok(i) == True:
            ls.append(i)
    for i in a:
        if csaknulla(i) == True:
            ls.append(i)
    return ls




